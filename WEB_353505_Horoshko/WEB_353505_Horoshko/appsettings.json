{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ItemsPerPage": 3,
  "UriData": {
    "ApiUri": "https://localhost:7002/api/"
  },
  "Keycloak": {
    "Host": "http://localhost:8080",
    "Realm": "Horoshko",
    "ClientId": "HoroshkoUiClient",
    "ClientSecret": "5LvPa7pM8p7IquBz2eySMQNFPwbpNxsU"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
